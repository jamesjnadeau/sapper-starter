{
  "env": {
    "node": true
  },
  "extends": "airbnb/base",
  "rules": {
    "no-var": [ 0 ],
    "vars-on-top": [ 0 ],
    "func-names": [ 0 ],
    "no-console": [ 0 ],
    "new-cap": [ 0 ],
    "no-alert": [ 0 ],
    "spaced-comment": [ 0 ],
    "quotes": [ 0 ],
    "object-shorthand": [ 0 ],
    "space-before-function-paren": [ 0 ],
    "prefer-arrow-callback": [ 0 ],
    "no-else-return": [ 0 ],
    "prefer-template": [ 0 ],
    "camelcase": [ 0 ],
    "one-var": [ 0 ],
    "no-param-reassign": [ 0 ],
    "consistent-return": [ 0 ]
  },
  "globals": {
    "$": true,
    "jQuery": true,
    "window.jQuery": true,
    "env": true
  }
}
